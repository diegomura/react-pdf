    "build": "rimraf ./lib && rollup -c",
    "watch": "rimraf ./lib && rollup -c -w"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@react-pdf/types": "^2.5.0",
    "cross-fetch": "^3.1.5",
    "fontkit": "^2.0.2",
    "is-url": "^1.2.4"
  },
  "files": [
    "lib"
  ]

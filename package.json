  "bundlesize": [
    {
      "path": "./dist/react-pdf.cjs.min.js",
      "maxSize": "17kB"
    },
    {
      "path": "./dist/react-pdf.browser.cjs.min.js",
      "maxSize": "17kB"
    }
  ],
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-pdf",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }

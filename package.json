    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.13.2",
    "jest": "^23.0.0",
    "lint-staged": "^3.4.2",
    "mock-fs": "^4.7.0",
    "mockdate": "^2.0.1",
    "prettier": "^1.6.1",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.52.2",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^2.7.1",
